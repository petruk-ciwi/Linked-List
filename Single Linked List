#include<iostream>
using namespace std;
struct node {
    int data;
	  node *next;
};
node *head, *tail;

int isEmpty() {
	if (head==NULL)
	return 1;
	else return 0;
}

void create() {
	node *baru=new node;
}

void InsFront(int nilai) {
	node *baru;
	baru =new node;
	baru->data=nilai;
	baru->next=NULL;
	if (isEmpty()==1){
		head=baru;
		head->next=NULL;
	} else {
		baru->next=head;
		head=baru;
	}
	cout<<"\nData "<<nilai<<" dimasukan ke dalam list\n";
}

